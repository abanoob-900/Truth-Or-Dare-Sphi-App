<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bob.truth_dare_sphi_app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="23"
        android:targetSdkVersion="33" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:extractNativeLibs="false"
        android:icon="@mipmap/ic_launcher_truth"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_truth"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.bob.truth_dare_sphi_app.StartActivity"
            android:exported="true"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.bob.truth_dare_sphi_app.DareActivity"
            android:exported="false"
            android:label="@string/dare" />
        <activity
            android:name="com.bob.truth_dare_sphi_app.TruthActivity"
            android:exported="false"
            android:label="@string/truth" />
        <activity
            android:name="com.bob.truth_dare_sphi_app.MainActivity"
            android:exported="false"
            android:screenOrientation="portrait" />
    </application>

</manifest>