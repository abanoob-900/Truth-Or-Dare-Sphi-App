<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bob.truth_dare_sphi_app">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_truth"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_truth"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".StartActivity"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DareActivity"
            android:exported="false"
            android:label="@string/dare"></activity>
        <activity
            android:name=".TruthActivity"
            android:exported="false"
            android:label="@string/truth"></activity>
        <activity
            android:name=".MainActivity"
            android:exported="false"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>